# Google Cloud Project Configuration
BQTAP_PROJECT_ID=your-project-id
BQTAP_DATASET=your-dataset-name
BQTAP_TABLE=console_logs

# Google Cloud credentials as JSON string
BQTAP_GOOGLE_API_CREDENTIALS='{"type":"service_account","project_id":"...","private_key":"..."}'

# Enable/disable BQTap (set to 'false' or '0' to disable)
BQTAP_ENABLED=true
